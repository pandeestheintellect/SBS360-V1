@model Eng360Web.Models.ViewModel.SupplierViewModel

    @{
        Layout = null;
    }

<div class="inner-page--container view">

    <div class="modal-footer">
        @*<button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>*@
        <input type="button" value="Back" id="create-supplier-dialog-save" class="btn btn-primary Supplier" />

    </div>

    <div class="panel panel-default">
        <div class="panel-heading">
            Supplier Details
        </div>

        <!-- /.panel-heading -->
        <div class="panel-body">
            <div class="modal-body">

                <table class="table table-bordered">
                    <tr>
                        <th bgcolor="beige">@Html.LabelFor(model => model.SupplierDisplayID) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.Company_Name) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.IndustryID) </th>
                    </tr>

                    <tr>
                        <td>@Html.DisplayFor(model => model.SupplierDisplayID)</td>
                        <td>@Html.DisplayFor(model => model.Company_Name)</td>
                        <td>@Html.DisplayFor(model => model.IndustryID)</td>
                    </tr>

                    <tr>
                        <th bgcolor="beige">@Html.LabelFor(model => model.Spoc_Name) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.Supplier_Description) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.eng_address_master.Email) </th>
                    </tr>

                    <tr>
                        <td>@Html.DisplayFor(model => model.Spoc_Name)</td>
                        <td>@Html.DisplayFor(model => model.Supplier_Description)</td>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Email)</td>
                    </tr>

                    <tr>
                        <th bgcolor="beige">@Html.LabelFor(model => model.eng_address_master.Mobile) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.eng_address_master.Tel) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.eng_address_master.Fax1) </th>
                    </tr>

                    <tr>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Mobile)</td>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Tel)</td>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Fax1)</td>
                    </tr>

                    <tr>
                        <th bgcolor="beige">@Html.LabelFor(model => model.eng_address_master.Address1) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.eng_address_master.Address2) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.eng_address_master.City) </th>
                    </tr>

                    <tr>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Address1)</td>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Address2)</td>
                        <td>@Html.DisplayFor(model => model.eng_address_master.City)</td>
                    </tr>

                    <tr>
                        <th bgcolor="beige">@Html.LabelFor(model => model.eng_address_master.Country) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.eng_address_master.Postal_Code) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.eng_address_master.SkypeID) </th>
                    </tr>

                    <tr>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Country)</td>
                        <td>@Html.DisplayFor(model => model.eng_address_master.Postal_Code)</td>
                        <td>@Html.DisplayFor(model => model.eng_address_master.SkypeID)</td>
                    </tr>

                    <tr>
                        <th bgcolor="beige">@Html.LabelFor(model => model.CreatedDate) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.CreatedBy) </th>
                        <th bgcolor="beige">@Html.DisplayNameFor(model => model.UpdatedDate) </th>
                    </tr>

                    <tr>
                        <td>@Html.DisplayFor(model => model.CreatedDate)</td>
                        <td>@Html.DisplayFor(model => model.CreatedBy)</td>
                        <td>@Html.DisplayFor(model => model.UpdatedDate)</td>
                    </tr>

                </table>

            </div>
        </div>
    </div>

    <div class="modal-footer">
        @*<button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>*@
        <input type="button" value="Back" id="create-supplier-dialog-save" class="btn btn-primary Supplier" />

    </div>

  </div>