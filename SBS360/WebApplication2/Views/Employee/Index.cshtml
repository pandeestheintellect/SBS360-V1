<script>
    $(document).ready(function () {
        debugger;
        $('#emplyeeTable').DataTable({
            "autoWidth": true,
            //"scrollX": true,
            dom: 'Bfrtipl',
            buttons: [
                {
                    extend: 'copyHtml5',
                    text: '<i class="fa fa-files-o"></i>',
                    titleAttr: 'Copy'
                },
                {
                    extend: 'excelHtml5',
                    text: '<i class="fa fa-file-excel-o"></i>',
                    titleAttr: 'Excel'
                },
                {
                    extend: 'csvHtml5',
                    text: '<i class="fa fa-file-text-o"></i>',
                    titleAttr: 'CSV'
                },
                {
                    extend: 'pdfHtml5',
                    text: '<i class="fa fa-file-pdf-o"></i>',
                    titleAttr: 'PDF'

                }

            ]
        });
    });
    $('.dt-buttons').prepend('<a  onclick="CreateEmployee()" class="dt-button buttons-create buttons-html5" tabindex="0" aria-controls="emplyeeTable" href="#" title="Create"><i class="fa fa-plus"></i></a>')
    $('.dt-button').addClass('btn btn-primary').removeClass('dt-button')

  $(function(){
        $('#testpanel, #testpanel1').lobiPanel({
            //Options go here             
            reload: false,
            close: false,
            editTitle: false
           
        });
    });
</script>